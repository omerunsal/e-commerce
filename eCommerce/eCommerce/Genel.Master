                            </AnonymousTemplate>

                            <LoggedInTemplate>
                                <asp:LoginStatus ID="LoginStatus1" runat="server" />
                                <asp:LoginName ID="LoginName1" runat="server" />
                            </LoggedInTemplate>

                        </asp:LoginView>
                        </div>
                        <br />
                        <div class="thumbnail">
