                            <asp:AdRotator ID="AdRotatorSolReklam" runat="server" AdvertisementFile="~/reklam.xml" Height="125px" Width="125px" />
                        </a>
                        </div>
                        <br />
                        <div class="thumbnail">
                           
                                <asp:LoginView  runat="server">
